  "source": "pragma solidity ^0.5.0;\n\ncontract TodoList {\n    // state variables\n    \n    uint8 public taskCount = 0;\n\n    struct Task {\n        uint8 id;\n        string content;\n        uint8 status;\n    }\n\n    mapping (uint=>Task) public tasks;\n\n    function createTask (string memory _content) public {\n        taskCount ++;\n        Task memory new_task = Task({\n            id: taskCount,\n            content: _content,\n            status: 0\n        });\n        tasks[taskCount] = new_task;\n    }\n}",
